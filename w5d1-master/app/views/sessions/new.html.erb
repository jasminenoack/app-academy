<h1>Log In</h1>
<%= flash[:error] %>

<form action="<%= session_url %>" method="post">
  <%= auth_token %>
  <label for="username">username
    <input id="username" type="text" name="username" value="<%= params[:username] %>">
  </label>

  <label>password
    <input type="password" name="password">
  </label>

  <button>Sign In</button>
</form>
