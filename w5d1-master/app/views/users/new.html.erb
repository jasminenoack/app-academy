<h1>New User</h1>

  <form action="<%= users_url %>" method="post">
    <%= auth_token %>
    <label for="username">username
      <input id="username" type="text" name="user[username]" value="<%= @user.username %>">
    </label>

    <label>password
      <input type="password" name="user[password]">
    </label>

    <button>Create User</button>
  </form>
